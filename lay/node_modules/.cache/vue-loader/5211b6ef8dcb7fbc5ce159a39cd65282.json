tChart() {\n            let option = {\n                title: {\n                    text: '【柱图数据标题】',\n                    textStyle: {\n                        color: '#75deef',\n                        fontSize: 12,\n                        fontWeight: 'normal'\n                    },\n                    top: '5%',\n                    left: '31%'\n                },\n                tooltip: {\n                    trigger: 'axis',\n                    axisPointer: {            // 坐标轴指示器，坐标轴触发有效\n                        type: 'none'        // 默认为直线，可选为：'line' | 'shadow'\n                    },\n                    backgroundColor: '#11367a',\n                    formatter: \"{b}:{c1}\",\n                    textStyle: {\n                        color: '#6dd0e3',\n                        fontSize: 10,\n                    },\n                },\n                grid:\n                    {\n                        top: '25%',\n                        left: '1%',\n                        right: '4%',\n                        bottom: '4%',\n                        containLabel: true,\n\n                    },\n                xAxis:\n                    {\n                        type: 'category',\n                        axisLabel: {\n                            interval: 0,\n                            color: '#75deef',\n                            fontSize: 9,\n                            showMinLabel: true\n                        },\n                        axisLine: {\n                            lineStyle: {\n                                color: '#1a3c58',\n                                width: 2\n                            }\n                        },\n                        axisTick: {show: false},\n\n                        data: ['数据1', '数据2', '数据3', '数据4', '数据5', '数据6', '数据7', '数据8']\n                    },\n                yAxis: [\n                    {\n                        type: 'value',\n                        min: 0,\n                  